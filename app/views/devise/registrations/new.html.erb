<div class="span5">
<h2>Sign up</h2>
	<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html:{ class: 'form-horizontal'}) do |f| %>
	<%= f.error_notification %>

	<%= f.input :user_name %>
	<%= f.input :first_name %>
	<%= f.input :last_name %>
	<%= f.input :email %>
	<%= f.input :password %>
	<%= f.input :password_confirmation %>

	<div class="form-actions">
		<%= f.submit "Sign up", class: "btn btn-large submit_button" %>
	</div>
<% end %>
</div>
<div class="clear"></div>